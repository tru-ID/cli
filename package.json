{
  "name": "@tru_id/cli",
  "description": "The tru.ID CLI",
  "version": "1.0.0",
  "author": {
    "name": "tru.ID",
    "email": "community@tru.id",
    "url": "https://tru.id"
  },
  "bin": {
    "tru": "bin/run"
  },
  "dependencies": {
    "@oclif/core": "^1.5.2",
    "@oclif/plugin-help": "^5.1.12",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "fast-json-patch": "^3.1.0",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.1",
    "get-port": "^5.1.1",
    "inquirer": "^8.2.0",
    "openid-client": "5.1.4",
    "phone": "^3.1.12",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/phone": "^2.4.1",
    "@types/sinon-chai": "^3.2.8",
    "chai": "^4",
    "eslint": "7",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "1.0.2",
    "eslint-config-prettier": "^8.4.0",
    "mocha": "^9.2.1",
    "mockdate": "3.0.5",
    "nyc": "^15.1.0",
    "oclif": "^2.5.0",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "ts-sinon": "^2.0.2",
    "typescript": "4.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "topics": {
      "projects": {
        "description": "Manage your Projects within your Workspace"
      },
      "phonechecks": {
        "description": "Manage PhoneChecks within a Project"
      },
      "oauth": {
        "description": "OAuth2 helpers. For example, creating Access Tokens."
      },
      "subscriberchecks": {
        "description": "Manage SubscriberChecks within a Project"
      },
      "simchecks": {
        "description": "Manage SIMChecks within a Project"
      },
      "usage": {
        "description": "Get daily, hourly, monthly usage"
      },
      "setup": {
        "description": "Setup oauth2 for cli"
      },
      "workspaces": {
        "description": "Switch workspaces and display info"
      }
    },
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "bin": "tru",
    "hooks": {
      "init": [
        "./dist/hooks/init/check-config"
      ]
    }
  },
  "repository": "tru-ID/tru-cli",
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true,
      "bump": true
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b && oclif manifest && oclif readme",
    "lint": "eslint . --ext .ts",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "release": "npm run version && standard-version --",
    "canary_version_suffix": "node scripts/canary-version-suffix.js",
    "npm:publish:dev": "npm publish --tag canary",
    "npm:publish:prod": "npm publish",
    "prettier": "prettier './**/*.{js,ts,json}' --write",
    "check-types": "tsc --noEmit --incremental false -p tsconfig.json --skipLibCheck"
  },
  "types": "dist/index.d.ts"
}
