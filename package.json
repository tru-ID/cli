{
  "name": "4auth-cli",
  "description": "The 4Auth CLI",
  "version": "0.1.1",
  "author": "Phil Leggetter @leggetter",
  "bin": {
    "4auth": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "axios": "^0.19.2",
    "cli-ux": "^5.4.9",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.2.0",
    "lookpath": "^1.1.0",
    "qrcode-terminal": "^0.12.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.5",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/sinon-chai": "^3.2.4",
    "aws-sdk": "^2.721.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "jest": "^26.1.0",
    "mocha": "^5",
    "nyc": "^14",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8",
    "ts-sinon": "^1.2.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "4auth",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/requireConfig"
      ]
    },
    "update": {
      "dev_s3": {
        "bucket": "4auth-cli-dev",
        "acl": "private"
      },
      "s3": {
        "bucket": "4auth-cli",
        "acl": "private"
      }
    }
  },
  "repository": "4auth/4auth-cli",
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "postinstall": "bin/postinstall.js",
    "postpack": "rm -f oclif.manifest.json",
    "pack": "oclif-dev pack",
    "x_posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "prerelease": "rm -f package-lock.json",
    "release": "npm run version && standard-version",
    "aws:publish": "bin/aws-publish.sh"
  },
  "types": "lib/index.d.ts"
}
